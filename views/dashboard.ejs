<!-- adds header.ejs -->
<%- include("partials/header") %>

<div class="container">
    <div class="row">
        <div class="col">
            <img width="100px" height="100px" style="border-radius: 50px" src=<%= data.displayImg %> alt=""> 
            <h1 class="text-center mt-5">Hello, <%= data.displayName %></h1><br>
        </div>
    </div>
  
    <div class="col">
        <h3 class="text-left mt-5">Find matches:</h3><br>
        <a class="btn btn-info btn-lg" href="/results" role="button">Show me matches</a>
        <a class="btn btn-outline-info btn-lg" href="#" role="button">Update search preferences</a>
    </div>
    
    <div class="col">
        <h3 class="text-left mt-5">Saved matches:</h3>
    </div>  
</div>

<!-- adds footer.ejs -->
<%- include("partials/footer") %>