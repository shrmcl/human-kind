<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.12.1/css/all.min.css" integrity="sha256-mmgLkCYLUQbXn0B1SRqzHar6dCnv9oZFPEC1g1cwlkk=" crossorigin="anonymous" />
  <link rel="stylesheet" href="stylesheets/chatbox.css">
  <title>ChatCord App</title>
</head>
<div class="chat-container">
    <header class="chat-header">
      <h1> ChatCord</h1>
      <a href="/results" class="btn">Leave Room</a>
    </header>
    <main class="chat-main">
      <div class="chat-sidebar">
        <!-- <h3><i class="fas fa-comments"></i> Room Name:</h3> -->
        <!-- <h2 id="room">Austin Pet Alive</h2> -->
        <h3><i class="fas fa-users"> Users</i></h3>
        <div class="col">
          <!-- <img class="profile-big" src=< %= data.displayImg %> alt="">  -->
          <!-- <h1 class="text-center mt-5"></h1><br> -->
        </div>
      </div>
      <div class="chat-messages"></div>
    </main>
    <div class="chat-form-container">
      <form id="chat-form">
        <input
          id="msg"
          type="text"
          placeholder="Enter Message"
          required
          autocomplete="off"
        />
        <button class="btn"><i class="fas fa-paper-plane"></i> Send</button>
      </form>
    </div>
  </div>

<%- include("partials/footer") %>